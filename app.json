{
  "expo": {
    "name": "EcoCollab",
    "slug": "EcoCollab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ecocollab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAS3d3T9sENBlWoop-edE_xv017wpvUqxE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.brunorito.EcoCollab",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAS3d3T9sENBlWoop-edE_xv017wpvUqxE"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "55444c3f-3b35-43e8-a10c-21b2988824cb"
      },
      "googleDirectionsKey": "AIzaSyAEoeJ6NYPDy7uO0jy3ahRSeO26VqavI4o"
    }
  }
}
